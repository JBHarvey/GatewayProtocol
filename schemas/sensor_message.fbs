namespace GatewayProtocol.Sensor;

table Track {
  id:uint16 = 0;
  distance:double = 0;
  intensity:double = 0;
  speed:double = 0;
  acceleration:double = 0;
  confidence_level:uint8 = 0;
} 

table Pixel {
  id:uint16 = 65535;
  tracks:[Track];
}

table Message {
  id:uint16 = 0;
  pixels:[Pixel];
}
