namespace GatewayProtocol.Sensor;

table Track {
  id:uint16 = 0;
  distance:uint16 = 0;
  intensity:uint16 = 0;
  speed:int16 = 0;
  acceleration:int16 = 0;
  confidence_level:uint16 = 0;
} 

table Pixel {
  id:uint16 = 65535;
  tracks:[Track];
}

table Message {
  id:uint16 = 0;
  pixels:[Pixel];
}
